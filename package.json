{
  "name": "react-source",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "private": "true",
  "scripts": {
    "dev": "vite --force",
    "demo": "vite serve examples/test-code --config scripts/vite/vite.config.js --force",
